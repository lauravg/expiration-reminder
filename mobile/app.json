{
  "expo": {
    "name": "PantryGuardian",
    "slug": "pantryguardian",
    "jsEngine": "jsc",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to scan food items.",
        "NSPhotoLibraryUsageDescription": "This app accesses the photo library to let you upload images."
      },
      "config": {
        "usesNonExemptEncryption": false
      }    
    },
    "android": {
      "package": "com.lauravh.pantryguardian",
      "versionCode": 1,
      "permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "scheme": "pantryguardian",
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "expoProjectId": "92e1251a-e072-484a-8ebd-10e717e57282",
      "eas": {
        "projectId": "2ce034a2-825e-4955-b0e9-d2924ccda867"
      }
    },
    "owner": "lauravh",
    "plugins": [
      "expo-secure-store"
    ]
  }
}
